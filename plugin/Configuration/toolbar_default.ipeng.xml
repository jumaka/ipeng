<?xml version="1.0" encoding="utf-8"?>
<configuration>
	<section id="toolbar">
		<subsection id="*">
			<command id="Album">
				<description>Toolbar Icon: Default Browse-By-Album View</description>
				<weight>20</weight>
				<namestring>ALBUMS</namestring>
				<icon>html/images/iAlbumTrans.png</icon>
				<selectedicon>html/images/iAlbumTransSelected.png</selectedicon>
				<url>clixmlbrowser/clicmd=browselibrary+items&amp;linktitle=BROWSE_BY_ALBUM&amp;mode=albums/?</url> 
				<parameters>
					<playerURI>player</playerURI>
				</parameters>
			</command>
			<command id="Artist">
				<description>Toolbar Icon: Default Browse-By-Artist View</description>
				<weight>21</weight>
				<namestring>ARTIST</namestring>
				<icon>html/images/iArtistTrans.png</icon>
				<selectedicon>html/images/iArtistTransSelected.png</selectedicon>
				<url>clixmlbrowser/clicmd=browselibrary+items&amp;linktitle=BROWSE_BY_ARTIST&amp;mode=artists/?</url> 
				<parameters>
					<playerURI>player</playerURI>
				</parameters>
			</command>
			<command id="Playlist">
				<description>Toolbar Icon: Default Browse-Playlists View</description>
				<weight>22</weight>
				<namestring>IPENG_BUTTON_LISTS</namestring>
				<icon>html/images/iPlaylistTrans.png</icon>
				<selectedicon>html/images/iPlaylistTransSelected.png</selectedicon>
				<url>clixmlbrowser/clicmd=browselibrary+items&amp;linktitle=SAVED_PLAYLISTS&amp;mode=playlists/?</url> 
				<parameters>
					<playerURI>player</playerURI>
				</parameters>
			</command>
			<command id="Search">
				<description>Toolbar Icon: Default Search View</description>
				<weight>23</weight>
				<namestring>SEARCH</namestring>
				<icon>html/images/iSearchTrans.png</icon>
				<selectedicon>html/images/iSearchTransSelected.png</selectedicon>
				<url>search.html?</url> 
				<parameters>
					<playerURI>player</playerURI>
				</parameters>
			</command>
			<command id="Home">
				<position>5</position>
				<description>Toolbar Icon: Home</description>
				<weight>25</weight>
				<namestring>IPENG_BUTTON_HOME</namestring>
				<icon>html/images/iHomeTrans.png</icon>
				<selectedicon>html/images/iHomeTransSelected.png</selectedicon>
				<url>home.html?</url> 
				<parameters>
					<playerURI>player</playerURI>
				</parameters>
			</command>
			<command id="Browsetree">
				<description>Toolbar Icon: Default Music Folder Browse View</description>
				<weight>25</weight>
				<defaultenabled>0</defaultenabled>
				<namestring>IPENG_BUTTON_TREE</namestring>
				<icon>html/images/iFolderTrans1.png</icon>
				<selectedicon>html/images/iFolderTrans1Selected.png</selectedicon>
				<url>browsetree.html?</url> 
				<parameters>
					<playerURI>player</playerURI>
				</parameters>
			</command>
			<command id="Year">
				<description>Toolbar Icon: Default Browse-By-Year View</description>
				<weight>25</weight>
				<defaultenabled>0</defaultenabled>
				<namestring>IPENG_BUTTON_YEAR</namestring>
				<icon>html/images/iTimeTrans.png</icon>
				<selectedicon>html/images/iTimeTransSelected.png</selectedicon>
				<url>clixmlbrowser/clicmd=browselibrary+items&amp;linktitle=BROWSE_BY_YEAR&amp;mode=years/?</url> 
				<parameters>
					<playerURI>player</playerURI>
				</parameters>
			</command>
			<command id="Genre">
				<description>Toolbar Icon: Default Browse-By-Genre View</description>
				<weight>25</weight>
				<defaultenabled>0</defaultenabled>
				<namestring>BROWSE_BY_GENRE</namestring>
				<icon>html/images/iGenreTrans.png</icon>
				<selectedicon>html/images/iGenreTransSelected.png</selectedicon>
				<url>clixmlbrowser/clicmd=browselibrary+items&amp;linktitle=BROWSE_BY_GENRE&amp;mode=genres/?</url> 
				<parameters>
					<playerURI>player</playerURI>
				</parameters>
			</command>
			<command id="Newmusic">
				<description>Toolbar Icon: Default Browse-New-Music View</description>
				<weight>25</weight>
				<defaultenabled>0</defaultenabled>
				<namestring>BROWSE_NEW_MUSIC</namestring>
				<icon>html/images/iNewTrans.png</icon>
				<selectedicon>html/images/iNewTransSelected.png</selectedicon>
				<url>clixmlbrowser/clicmd=browselibrary+items&amp;linktitle=BROWSE_NEW_MUSIC&amp;sort=new&amp;wantMetadata=1&amp;mode=albums/?</url> 
				<parameters>
					<playerURI>player</playerURI>
				</parameters>
			</command>
			<command id="RandomPlay">
				<description>Toolbar Icon: Random Play</description>
				<weight>25</weight>
				<defaultenabled>0</defaultenabled>
				<namestring>PLUGIN_RANDOMPLAY</namestring>
				<requireplugins>RandomPlay</requireplugins>
				<icon>html/images/iRandomTrans.png</icon>
				<selectedicon>html/images/iRandomTransSelected.png</selectedicon>
				<url>plugins/RandomPlay/list.html?</url> 
				<parameters>
					<playerURI>player</playerURI>
				</parameters>
			</command>
			<command id="Favorites">
				<description>Toolbar Icon: Favorites</description>
				<weight>25</weight>
				<defaultenabled>0</defaultenabled>
				<namestring>FAVORITES</namestring>
				<requireplugins>Favorites</requireplugins>
				<icon>html/images/iFavoritesTrans.png</icon>
				<selectedicon>html/images/iFavoritesTransSelected.png</selectedicon>
				<url>plugins/Favorites/index.html?</url> 
				<parameters>
					<playerURI>player</playerURI>
				</parameters>
			</command>
			<command id="Podcast">
				<description>Toolbar Icon: Podcast</description>
				<weight>25</weight>
				<defaultenabled>0</defaultenabled>
				<namestring>PLUGIN_PODCAST</namestring>
				<requireplugins>Podcast</requireplugins>
				<icon>html/images/iPodcastTrans.png</icon>
				<selectedicon>html/images/iPodcastTransSelected.png</selectedicon>
				<url>plugins/Podcast/index.html?</url> 
				<parameters>
					<playerURI>player</playerURI>
				</parameters>
			</command>
			<command id="LastFM">
				<description>Toolbar Icon: LastFM</description>
				<weight>25</weight>
				<defaultenabled>0</defaultenabled>
				<name>LastFM</name>
				<requireplugins>SqueezeScrobbler</requireplugins>
				<icon>plugins/LastFM/html/images/lastfmTrans.png</icon>
				<selectedicon>plugins/LastFM/html/images/lastfmTransSelected.png</selectedicon>
				<url>plugins/LastFM/index.html?</url> 
				<parameters>
					<playerURI>player</playerURI>
				</parameters>
			</command>
			<command id="XMRadio">
				<description>Toolbar Icon: XM Radio</description>
				<weight>25</weight>
				<defaultenabled>0</defaultenabled>
				<name>XM Radio</name>
				<requireplugins>XMRadio</requireplugins>
				<icon>plugins/XMRadio/html/images/XMTrans.png</icon>
				<selectedicon>plugins/XMRadio/html/images/XMTransSelected.png</selectedicon>
				<url>plugins/XMRadio/index.html?autologin=1</url> 
				<parameters>
					<playerURI>player</playerURI>
				</parameters>
			</command>
			<command id="Pandora">
				<description>Toolbar Icon: Pandora</description>
				<weight>25</weight>
				<defaultenabled>0</defaultenabled>
				<name>Pandora</name>
				<requireplugins>Pandora</requireplugins>
				<icon>plugins/Pandora/html/images/PandoraTrans.png</icon>
				<selectedicon>plugins/Pandora/html/images/PandoraTransSelected.png</selectedicon>
				<url>plugins/pandora/index.html?</url> 
				<parameters>
					<playerURI>player</playerURI>
				</parameters>
			</command>
			<command id="Power">
				<description>Toolbar Icon: Power</description>
				<defaultenabled>0</defaultenabled>
				<position>1</position>
				<weight>25</weight>
				<namestring>IPENG_POWER</namestring>
				<icon>html/images/iOffTrans.png</icon>
				<selectedicon>html/images/iOffBlue.png</selectedicon>
				<cli>power</cli>
			</command>
			<command id="PlayPause">
				<description>Toolbar Icon: Play / Pause</description>
				<defaultenabled>0</defaultenabled>
				<weight>25</weight>
				<position>3</position>
				<namestring>IPENG_PLAY_PAUSE</namestring>
				<icon>html/images/iPlayPause.png</icon>
				<cli>pause</cli>
			</command>
			<command id="Next">
				<description>Toolbar Icon: Next Track</description>
				<defaultenabled>0</defaultenabled>
				<weight>25</weight>
				<position>4</position>
				<namestring>NEXT</namestring>
				<icon>html/images/iNextTrans.png</icon>
				<cli>playlist</cli>
				<parameters>
					<other>index</other>
					<other1>+1</other1>
				</parameters>
			</command>
			<command id="Prev">
				<description>Toolbar Icon: Previous Track</description>
				<defaultenabled>0</defaultenabled>
				<weight>25</weight>
				<position>2</position>
				<namestring>PREV</namestring>
				<icon>html/images/iPrevTrans.png</icon>
				<cli>playlist</cli>
				<parameters>
					<other>index</other>
					<other1>-1</other1>
				</parameters>
			</command>
		</subsection>
		<subsection id="home">
